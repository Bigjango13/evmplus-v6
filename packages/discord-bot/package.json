{
	"name": "discord-bot",
	"version": "1.17.0",
	"description": "Manages members in the Discord server used by CAP St. Mary's",
	"main": "dist/index.js",
	"scripts": {
		"test": "jest",
		"build": "ttsc --build tsconfig.build.json",
		"watch": "ttsc --build tsconfig.build.json --watch",
		"check": "ttsc --build tsconfig.build.json --noEmit",
		"lint": "tslint --project ."
	},
	"private": true,
	"author": "Andrew Rioux",
	"license": "GPL-2.0",
	"jest": {
		"globals": {
			"ts-jest": {
				"tsConfig": "tsconfig.test.json",
				"diagnostics": false
			}
		},
		"testRegex": "(/src/__tests__/.*\\.ts)",
		"testPathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/src/__tests__/consts.ts",
			"<rootDir>/src/__tests__/TestUtils.ts",
			"<rootDir>/src/__tests__/EitherMatcher.ts"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleFileExtensions": [
			"ts",
			"js",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@types/debug": "^4.1.5",
		"@types/express": "^4.17.2",
		"@types/jest": "^26",
		"@types/node": "^13.13.4",
		"@types/ws": "^7.2.9",
		"jest": "^26",
		"jest-cli": "^26",
		"jest-watch-typeahead": "^0.6.1",
		"prettier": "^2.0.5",
		"ts-jest": "^26",
		"tslint": "^6.0.0",
		"tslint-config-prettier": "^1.18.0",
		"ttypescript": "^1.5.10",
		"typescript": "^4.0.5"
	},
	"dependencies": {
		"@mysql/xdevapi": "^8.0.19",
		"auto-client-api": "^1.3.21",
		"common-lib": "^1.26.0",
		"debug": "^4.3.1",
		"discord.js": "^12.5.1",
		"dotenv": "^8.2.0",
		"server-common": "^1.23.0"
	}
}
