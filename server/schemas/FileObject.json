{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Represents a file. Metadata (shown) is stored in the database, other file\ndata is stored on disk to take advantage of Node.js",
    "properties": {
        "_id": {
            "description": "The ID assigned by NoSQL/MySQL",
            "type": "string"
        },
        "accountID": {
            "description": "The ID of the account the folder belongs to",
            "type": "string"
        },
        "comments": {
            "description": "Comments about the file",
            "type": "string"
        },
        "contentType": {
            "description": "The MIME type for the file",
            "type": "string"
        },
        "created": {
            "description": "The UTC unix time stamp of when the file was created",
            "type": "number"
        },
        "fileChildren": {
            "description": "Children ids for folder",
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "fileName": {
            "description": "The name of the file",
            "type": "string"
        },
        "forDisplay": {
            "description": "Whether or not the file is displayed in the photo library (only works with photos)",
            "type": "boolean"
        },
        "forSlideshow": {
            "description": "Whether or not the file is to be shown in the slideshow",
            "type": "boolean"
        },
        "id": {
            "description": "The file identifier",
            "type": "string"
        },
        "kind": {
            "description": "Prevent duck typing to an extent",
            "enum": [
                "drive#file"
            ],
            "type": "string"
        },
        "memberOnly": {
            "description": "Whether or not the file is limited to the members",
            "type": "boolean"
        },
        "parentID": {
            "description": "ID of the parent for going backwards",
            "type": "string"
        },
        "uploaderID": {
            "description": "The id of the uploader",
            "type": "number"
        }
    },
    "required": [
        "accountID",
        "comments",
        "contentType",
        "created",
        "fileChildren",
        "fileName",
        "forDisplay",
        "forSlideshow",
        "id",
        "kind",
        "memberOnly",
        "parentID",
        "uploaderID"
    ],
    "type": "object"
}

