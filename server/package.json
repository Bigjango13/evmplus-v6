{
  "name": "capunit-com-server",
  "version": "4.0.0-alpha",
  "private": true,
  "scripts": {
    "start": "dist/index.js",
    "sass": "sass --watch ./client/src/nhq.sass --style=compressed",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "build": "tsc",
    "debug": "nodemon --inspect dist/index.js",
    "check": "tsc --noEmit",
    "clean-watch": "onchange '**/*.ts' -- prettier --write {{changed}}"
  },
  "main": "dist/index.js",
  "dependencies": {
    "aws-sdk": "^2.610.0",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "common-lib": "file:../lib",
    "csv-parse": "^2.5.0",
    "debug": "^2.6.9",
    "error-stack-parser": "^2.0.2",
    "exif": "^0.6.0",
    "express": "^4.16.3",
    "googleapis": "^47.0.0",
    "jsonwebtoken": "^8.2.0",
    "luxon": "^1.3.1",
    "mime-types": "^2.1.19",
    "morgan": "^1.9.1",
    "onchange": "^6.1.0",
    "pdfkit": "^0.10.0",
    "prettier": "^1.19.1",
    "promise-mysql": "^3.2.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.4.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@mysql/xdevapi": "^8.0.16",
    "@types/body-parser": "^1.16.8",
    "@types/cheerio": "^0.22.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/csv-parse": "^1.1.11",
    "@types/debug": "^0.0.30",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.16.0",
    "@types/gulp-tslint": "^4.2.0",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/luxon": "^1.2.1",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^13.7.0",
    "@types/pdfkit": "^0.10.1",
    "@types/react": "^16.8.4",
    "@types/request-promise": "^4.1.44",
    "@types/request-promise-native": "^1.0.15",
    "@types/supertest": "^2.0.5",
    "@types/uuid": "^3.4.3",
    "express-unit": "^2.1.1",
    "gulp": "^4",
    "gulp-shell": "^0.7.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "nodemon": "^1.19.4",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json",
        "diagnostics": false
      }
    },
    "testRegex": "(/src/__tests__/.*\\.ts)",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/__tests__/consts.ts",
      "<rootDir>/src/__tests__/TestUtils.ts",
      "<rootDir>/src/__tests__/EitherMatcher.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
